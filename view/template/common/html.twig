<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <base href="{{ base }}">
    <title>{{ title }}</title>
    <meta name="description" content="{{ description }}">
    <meta name="keywords" content="{{ keywords }}">
    <link rel="icon" href="img/icon/favicons/favicon.svg" type="image/svg+xml">
    {% if template_bootstrap %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    {% endif %}
    <!-- Подключение styles -->
    {% for item in styles %}
        <link rel="{{ item.rel }}" href="{{ item.href }}" media="{{ item.media }}">
    {% endfor %}
    <!-- Подключение meta-teg`s -->
    {% for tag in meta_tags %}
        <meta {{ tag }}>
    {% endfor %}
    <!-- Подключение Link`s -->
    {% for item in links %}
        <link rel="{{ item.rel }}" href="{{ item.href }}">
    {% endfor %}
    <!-- Подключение JavaScript`s -->
    {% for item in js_head %}
        <script src="{{ item }}" type="text/javascript"></script>
    {% endfor %}
    {% for item in js_code_head %}
        <script type="text/javascript">{{ item }}</script>
    {% endfor %}
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.10.3/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.10.3/themes/sunny/jquery-ui.css">
</head>
<body>
    <!-- Подключение Шапки -->
    {{header}}
    <!-- Подключение Контента -->
    <div class="content">
    {{content}}
    </div>
    <!-- Подключение Подвала -->
    {{footer}}
    <!-- Подключение JavaScript`s -->
    {% if template_bootstrap %}
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    {% endif %}
    {% for item in js_body %}
        <script src="{{ item }}" type="text/javascript"></script>
    {% endfor %}
    {% for item in js_code_body %}
        <script>{{ item }}</script>
    {% endfor %}
</body>
</html>